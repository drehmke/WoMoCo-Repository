<body ng-controller="AccountController as account">
    <div class="container">
        <h1>WoMoCo</h1>
        <div>
          <a href="/">Home</a> /
            <a href="/about">About</a>/
            <a href="/interest">Interest</a>/          
            <a href="/admin/calendar/allEvents"  ng-show="account.isLoggedIn()">Event Admin</a>/            
            <a href="/post">Post</a>/
            <a href="/editingPost">EditingPost</a>/
            <a href="/secret">Secret</a>/      
            <a href="/message/inbox" ng-show="account.isLoggedIn()"> Inbox</a>/            
            <a href="/admin/calender/allEvents"  ng-show="account.isLoggedIn()">Event Admin /</a>
            <a href="/profile" ng-show="account.isLoggedIn()">Profile</a>/           
            <a href="/search">Search</a> /
            <a href="/interest">Interest</a>/                       
            <a href="/message/modal/messageModal">Message Modal</a>
            <a href="/link">Link</a> /
            <a href="/babySitterlink">BabySitterLink</a>/
           <a href="/login" ng-show="!account.isLoggedIn()">Login</a>/
           <a href="#" ng-click="account.logout()" ng-show="account.isLoggedIn()">Logout</a>
           <div ng-show="account.isLoggedIn()">
               [{{account.getUserName()}}]
           </div>
        </div>
        <hr />

        <ui-view></ui-view>
    </div>

